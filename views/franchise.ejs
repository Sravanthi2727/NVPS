  <!-- Enquiry Form Section -->
  <section class="franchise-form-section" id="franchise-form" aria-labelledby="form-title" itemscope itemtype="https://schema.org/ContactPage">
    <div class="container">
      <header class="franchise-form-header">
        <h2 id="form-title" class="franchise-form-title" itemprop="name">Start Your Franchise Journey</h2>
        <p class="franchise-form-subtitle" itemprop="description">Ready to bring Rabuste Coffee to your community? Tell us about your vision and we'll guide you through the next steps.</p>
      </header>
      
      <div class="franchise-form-container">
        <form class="franchise-form" id="franchiseEnquiryForm" novalidate itemscope itemtype="https://schema.org/ContactForm">
          <!-- Full Name Field -->
          <div class="franchise-form-group">
            <label for="fullName" class="franchise-form-label">Full Name *</label>
            <input 
              type="text" 
              id="fullName" 
              name="fullName" 
              class="franchise-form-input" 
              placeholder="Enter your full name"
              required
              aria-describedby="fullName-error"
              itemprop="name"
            >
            <div class="franchise-form-error" id="fullName-error" role="alert"></div>
          </div>

          <!-- Email Field -->
          <div class="franchise-form-group">
            <label for="email" class="franchise-form-label">Email Address *</label>
            <input 
              type="email" 
              id="email" 
              name="email" 
              class="franchise-form-input" 
              placeholder="your.email@example.com"
              required
              aria-describedby="email-error"
              itemprop="email"
            >
            <div class="franchise-form-error" id="email-error" role="alert"></div>
          </div>

          <!-- Phone Field -->
          <div class="franchise-form-group">
            <label for="phone" class="franchise-form-label">Phone Number *</label>
            <input 
              type="tel" 
              id="phone" 
              name="phone" 
              class="franchise-form-input" 
              placeholder="(555) 123-4567"
              required
              aria-describedby="phone-error"
              itemprop="telephone"
            >
            <div class="franchise-form-error" id="phone-error" role="alert"></div>
          </div>

          <!-- City & State Field -->
          <div class="franchise-form-group">
            <label for="cityState" class="franchise-form-label">City & State *</label>
            <input 
              type="text" 
              id="cityState" 
              name="cityState" 
              class="franchise-form-input" 
              placeholder="e.g., Austin, TX"
              required
              aria-describedby="cityState-error"
              itemprop="addressLocality"
            >
            <div class="franchise-form-error" id="cityState-error" role="alert"></div>
          </div>

          <!-- Investment Range Field -->
          <div class="franchise-form-group">
            <label for="investmentRange" class="franchise-form-label">Investment Range *</label>
            <select 
              id="investmentRange" 
              name="investmentRange" 
              class="franchise-form-select" 
              required
              aria-describedby="investmentRange-error"
            >
              <option value="">Select your investment range</option>
              <% investmentRanges.forEach(range => { %>
                <option value="<%= range %>"><%= range %></option>
              <% }); %>
            </select>
            <div class="franchise-form-error" id="investmentRange-error" role="alert"></div>
          </div>

          <!-- Message Field -->
          <div class="franchise-form-group">
            <label for="message" class="franchise-form-label">Tell Us About Your Vision</label>
            <textarea 
              id="message" 
              name="message" 
              class="franchise-form-textarea" 
              placeholder="Share your experience, goals, and what excites you about bringing Rabuste Coffee to your community..."
              rows="4"
              maxlength="500"
              aria-describedby="message-counter"
              itemprop="message"
            ></textarea>
            <div class="franchise-form-counter" id="message-counter">0 / 500 characters</div>
          </div>

          <!-- Acknowledgment Checkbox -->
          <div class="franchise-form-group franchise-form-checkbox-group">
            <div class="franchise-form-checkbox-wrapper">
              <input 
                type="checkbox" 
                id="acknowledgment" 
                name="acknowledgment" 
                class="franchise-form-checkbox" 
                required
                aria-describedby="acknowledgment-error"
              >
              <label for="acknowledgment" class="franchise-form-checkbox-label">
                I understand this is a premium Robusta-only concept and I'm interested in learning more about the unique positioning and requirements.
              </label>
            </div>
            <div class="franchise-form-error" id="acknowledgment-error" role="alert"></div>
          </div>

          <!-- Submit Button -->
          <div class="franchise-form-submit">
            <button type="submit" class="btn-franchise-submit" id="submitButton">
              <span class="btn-text">Start My Franchise Journey</span>
              <span class="btn-loading" style="display: none;">
                <svg class="btn-spinner" width="24" height="24" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg" aria-hidden="true">
                  <circle cx="12" cy="12" r="10" stroke="currentColor" stroke-width="2" stroke-linecap="round" stroke-dasharray="31.416" stroke-dashoffset="31.416">
                    <animate attributeName="stroke-dasharray" dur="2s" values="0 31.416;15.708 15.708;0 31.416" repeatCount="indefinite"/>
                    <animate attributeName="stroke-dashoffset" dur="2s" values="0;-15.708;-31.416" repeatCount="indefinite"/>
                  </circle>
                </svg>
                Submitting...
              </span>
            </button>
          </div>

          <!-- Success Message -->
          <div class="franchise-form-success" id="successMessage" style="display: none;" role="alert" aria-live="polite">
            <div class="franchise-form-success-icon" aria-hidden="true">
              <svg width="48" height="48" viewBox="0 0 48 48" fill="none" xmlns="http://www.w3.org/2000/svg">
                <circle cx="24" cy="24" r="20" fill="#228B22"/>
                <path d="M16 24L21 29L32 18" stroke="white" stroke-width="3" stroke-linecap="round" stroke-linejoin="round"/>
              </svg>
            </div>
            <h3 class="franchise-form-success-title">Thank You for Your Interest!</h3>
            <p class="franchise-form-success-message">
              We've received your franchise enquiry and are excited about the possibility of partnering with you. 
              Our franchise development team will review your information and contact you within 2 business days to discuss the next steps.
            </p>
            <p class="franchise-form-success-next">
              In the meantime, feel free to <a href="#" class="franchise-form-success-link">download our franchise deck</a> 
              to learn more about the Rabuste Coffee opportunity.
            </p>
          </div>
        </form>
      </div>
    </div>
  </section>

