  <!-- Qualification Section -->
  <section class="qualification-section">
    <div class="container">
      <h2 class="section-title text-center">Are You Ready to Partner with Us?</h2>
      <p class="section-subtitle text-center">We're looking for passionate entrepreneurs who share our vision</p>
      
      <div class="qualification-grid">
        <div class="qualification-item">
          <div class="qualification-icon">
            <svg width="48" height="48" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
              <path d="M12 2L15.09 8.26L22 9.27L17 14.14L18.18 21.02L12 17.77L5.82 21.02L7 14.14L2 9.27L8.91 8.26L12 2Z" stroke="#D4AF37" stroke-width="2"/>
            </svg>
          </div>
          <h3>Financial Capability</h3>
          <p>Liquid capital of ₹50-75L and total investment capacity of ₹75L-₹1.5Cr</p>
        </div>
        
        <div class="qualification-item">
          <div class="qualification-icon">
            <svg width="48" height="48" viewBox="0 0 24 24" fill="none" xmlns="http://www.w3.org/2000/svg">
              <path d="M16 21V19C16 16.7909 14.2091 15 12 15H5C2.79086 15 1 16.7909 1 19V21" stroke="#D4AF37" stroke-width="2"/>
              <circle cx="8.5" cy="7" r="4" stroke="#D4AF37" stroke-width="2"/>
