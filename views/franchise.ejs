  <!-- Support & Training Section -->
  <section class="support-section">
    <div class="container">
      <h2 class="section-title text-center">Complete Support System</h2>
      <p class="section-subtitle text-center">We're with you every step of the way</p>
      
      <div class="support-timeline">
        <div class="timeline-item">
